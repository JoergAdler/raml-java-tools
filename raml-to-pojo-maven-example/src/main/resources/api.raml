types:
    foo:
      (ramltopojo.types):
        plugins:
          - name: core.repackage
            arguments: [bloobloo]
      type: object
      discriminatorValue: myOwnValue
      properties:
        name: string
        goo:
          type: object
          properties:
            glue: boolean
            flue:
              type: object
              properties:
                another: string
    eater:
      (ramltopojo.types):
        plugins:
          - name: core.repackage
            arguments: [haha]
      type: object
      discriminatorValue: myOwnValue
      properties:
        name: string
        other: foo

