#%RAML 1.0
title: Hello World API
version: v1
baseUri: https://api.github.com
uses:
  ramltopojo: ramltopojo.raml
#(ramltopojo.types):
#  plugins:
#    - name: core.splitInterface
#      arguments: [com.fun, com.fun.impl]
#    - name: core.jackson2
